{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/product.schema.json",
  "title": "Articles from Telex",
  "type": "object",
  "required": "[ID]",
  "properties": {
    "ID": {
      "type": "string",
      "identity": 1
    },
    "Article": {
      "type": "object",
      "properties": {
        "ID": {
          "type": "number"
        },
        "Title": {
          "type": "string"
        },
        "Summary": {
          "type": "string"
        },
        "PublicationDate": {
          "type": "string"
        }
      }
    }
  }
}